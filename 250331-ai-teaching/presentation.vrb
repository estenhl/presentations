\begin{lstlisting}
			\documentclass{beamer}
			\usepackage{listings}

			\lstset{
				basicstyle=\fontencoding{T1}\ttfamily\tiny,
				numbers=left,
				numberstyle=\tiny,
				frame=single,
				breaklines=true,
				breakatwhitespace=true,
				tabsize=4,
				commentstyle=\color{gray},
				keywordstyle=\color{blue},
				stringstyle=\color{red},
				language={[LaTeX]TeX}
			}

			\begin{document}



